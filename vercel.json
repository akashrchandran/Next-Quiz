{
    "routes": [
      {
        "src": "/api/.*",
        "dest": "express/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "nextapp/pages/$1"
      }
    ],
    "builds": [
      { "src": "express/server.js", "use": "@vercel/node" },
      { "src": "nextapp/next.config.js", "use": "@vercel/next" }
    ]
  }